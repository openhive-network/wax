{
  "name": "",
  "version": "",
  "description": "call hived functions from JavaScript",
  "main": "wasm/dist/wax.js",
  "type": "module",
  "private": false,
  "scripts": {
    "prebuild": "sh scripts/compile_proto_ts.sh",
    "build": "rimraf wasm/dist && tsc",
    "postbuild": "fix-esm-import-path wasm/dist/proto",
    "test": "node wasm/wax_wasm_test/run_node.mjs",
    "examples": "cd examples/ts && npm install && npm run examples && cd ../.."
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.0",
    "fix-esm-import-path": "^1.4.0",
    "jest": "^29.7.0",
    "jest-puppeteer": "^9.0.0",
    "puppeteer": "^21.3.5",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "ts-proto": "^1.156.7",
    "typescript": "4.4.4"
  },
  "dependencies": {
    "protobufjs": "^7.2.5"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "files": [
    "build_wasm/wax_wasm.js",
    "build_wasm/wax_wasm.wasm",
    "build_wasm/wax.d.ts",
    "wasm/dist"
  ],
  "types": "wasm/dist/wax.d.ts",
  "repository": {
    "type": "git",
    "url": "https://gitlab.syncad.com/hive/wax.git"
  },
  "engines": {
    "node": ">= 12"
  },
  "publishConfig": {
    "registry": "",
    "tag": ""
  },
  "jest": {
    "preset": "jest-puppeteer",
    "runInBand": true,
    "bail": true,
    "testMatch": [ "**/?(*.)+(spec|test).[t]s" ],
    "testPathIgnorePatterns": ["/node_modules/", "dist"],
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    }
  }
}
